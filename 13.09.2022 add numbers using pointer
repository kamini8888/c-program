#include<stdio.h>
#include<conio.h>
int main()
{
    int num1, num2, sum;
    int *p1, *p2;
    printf("Enter any two Number: ");
    scanf("%d%d", &num1, &num2);
    p1 = &num1;
    p2 = &num2;
    sum = *p1 + *p2;
    printf("\nSum of %d and %d is %d", *p1, *p2, sum);
    getch();
    return 0;
}
